<h1>CSV File Import</h1>

<% if flash[:notice].present? %>
  <div class="alert alert-info" role="alert">
    <%= flash[:notice] %>
  </div>
<% end %>


<%= form_tag csv_import_upload_path, multipart: true do%>
  <%= file_field_tag :csv_file %>
  <%= submit_tag "Process CSV", class: 'btn btn-sm btn-primary' %>
<% end -%>
